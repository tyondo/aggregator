{
	"name": "tyondo/aggregator",
	"description": "A simple blog package",
	"keywords": ["blog", "tyondo"],
	"type": "library",
	"minimum-stability": "dev",
	"license": "MIT",
	"homepage": "https://github.com/Rndwiga/mnara",
	"authors": [
		{
			"name": "Raphael Ndwiga",
			"email": "packages@tyondo.com"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"illuminate/support": "5.3.*|5.4.*|5.5.*",
		"laravelcollective/html": "5.3.1|5.4.1",
		"unisharp/laravel-filemanager": "^1.8",
		"intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
		"maatwebsite/excel": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.5"
	},
	"suggest": {
		"tyondo/mnara": "User management package with ACL for managing Users / Roles / Permissions in Laravel 5.3 and 5.4."
	},
	"autoload": {
		"psr-4": {
			"Tyondo\\Aggregator\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Tyondo\\Aggregator\\TyondoAggregatorServiceProvider"
			]
		}
	}
}

